{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "module": "metadata_backend",
            "console": "integratedTerminal",
            "autoReload": {
                "enable": true
            },
            "restart": true,
            "remoteRoot": "/usr/local/lib/python3.8/site-packages/metadata_backend",
            "localRoot": "${workspaceFolder}/metadata_backend/"
        }
    ]
}

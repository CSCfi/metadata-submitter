{
    "title": "Analysis",
    "definitions": {
        "xrefLink": {
            "$id": "#/definitions/xrefLink",
            "type": "array",
            "title": "XRef Link",
            "items": {
                "type": "object",
                "required": [
                    "db",
                    "id"
                ],
                "properties": {
                    "db": {
                        "type": "string",
                        "title": "Database"
                    },
                    "id": {
                        "type": "string",
                        "title": "Associated accession Id"
                    }
                }
            }
        },
        "urlLink": {
            "$id": "#/definitions/urlLink",
            "type": "array",
            "title": "URL Link",
            "items": {
                "type": "object",
                "required": [
                    "label",
                    "url"
                ],
                "properties": {
                    "label": {
                        "type": "string",
                        "title": "Label",
                        "description": "Text label to display for the link."
                    },
                    "url": {
                        "type": "string",
                        "title": "URL",
                        "description": "The internet service link (http(s), ftp) etc.",
                        "pattern": "^(https?|ftp)://"
                    }
                }
            }
        },
        "entrezLink": {
            "$id": "#/definitions/entrezLink",
            "type": "array",
            "title": "Entrez Link",
            "items": {
                "type": "object",
                "required": [
                    "db"
                ],
                "properties": {
                    "db": {
                        "description": "NCBI controlled vocabulary of permitted cross references. Please see http://www.ncbi.nlm.nih.gov/entrez/eutils/einfo.fcgi? .",
                        "title": "Database",
                        "type": "string"
                    },
                    "label": {
                        "description": "How to label the link.",
                        "title": "Label",
                        "type": "string"
                    }
                }
            }
        },
        "analysisAttribute": {
            "$id": "#/definitions/analysisAttribute",
            "type": "object",
            "title": "Analysis Attribute",
            "description": "Properties and attributes of the data set. These can be entered as free-form tag-value pairs. Submitters may be asked to follow a community established ontology when describing the work.",
            "required": [
                "tag",
                "value"
            ],
            "properties": {
                "tag": {
                    "type": "string",
                    "title": "Tag title"
                },
                "value": {
                    "type": "string",
                    "title": "Description"
                }
            }
        },
        "sequenceType": {
            "$id": "#/definitions/sequenceType",
            "type": "object",
            "required": [
                "assembly",
                "sequence"
            ],
            "properties": {
                "assembly": {
                    "type": "object",
                    "title": "Assembly",
                    "properties": {
                        "standard": {
                            "type": "object",
                            "title": "Standard",
                            "required": [
                                "accessionId"
                            ],
                            "properties": {
                                "refname": {
                                    "type": "string",
                                    "title": "Reference name"
                                },
                                "accessionId": {
                                    "type": "string",
                                    "title": "Accession Id"
                                }
                            }
                        }
                    }
                },
                "sequence": {
                    "type": "array",
                    "title": "Sequence",
                    "items": {
                        "type": "object",
                        "required": [
                            "accessionId"
                        ],
                        "additionalProperties": true,
                        "properties": {
                            "label": {
                                "type": "string",
                                "title": "Label"
                            },
                            "accessionId": {
                                "type": "string",
                                "title": "Accession Id"
                            }
                        }
                    }
                }
            }
        },
        "reference": {
            "$id": "#/definitions/reference",
            "additionalProperties": true,
            "type": "object",
            "required": [
                "accessionId"
            ],
            "properties": {
                "label": {
                    "type": "string",
                    "title": "Label"
                },
                "refname": {
                    "type": "string",
                    "title": "Record name"
                },
                "refcenter": {
                    "type": "string",
                    "title": "Center Namespace"
                },
                "accessionId": {
                    "type": "string",
                    "title": "Accession Id"
                },
                "identifiers": {
                    "type": "object",
                    "title": "identifiers",
                    "properties": {
                        "primaryId": {
                            "type": "string",
                            "title": "Primary Id"
                        },
                        "submitterId": {
                            "type": "object",
                            "title": "Submitter Id",
                            "required": [
                                "namespace",
                                "value"
                            ],
                            "additionalProperties": true,
                            "properties": {
                                "namespace": {
                                    "type": "string",
                                    "title": "Namespace"
                                },
                                "value": {
                                    "type": "string",
                                    "title": "Value"
                                }
                            }
                        }
                    }
                }
            }
        },
        "file": {
            "$id": "#/definitions/file",
            "type": "object",
            "title": "File",
            "required": [
                "filename",
                "filetype",
                "checksumMethod",
                "checksum"
            ],
            "properties": {
                "filename": {
                    "type": "string",
                    "title": "Filename"
                },
                "filetype": {
                    "type": "string",
                    "title": "Filetype",
                    "enum": [
                        "tab",
                        "bam",
                        "bai",
                        "cram",
                        "crai",
                        "vcf",
                        "vcf_aggregate",
                        "bcf",
                        "tabix",
                        "wig",
                        "bed",
                        "gff",
                        "fasta",
                        "fastq",
                        "flatfile",
                        "chromosome_list",
                        "sample_list",
                        "readme_file",
                        "phenotype_file",
                        "BioNano_native",
                        "Kallisto_native",
                        "agp",
                        "unlocalised_list",
                        "info",
                        "manifest",
                        "other"
                    ]
                },
                "checksumMethod": {
                    "type": "string",
                    "title": "Checksum Method",
                    "enum": [
                        "MD5",
                        "SHA256"
                    ]
                },
                "checksum": {
                    "type": "string",
                    "title": "Checksum"
                }
            }
        }
    },
    "type": "object",
    "description": "A SRA analysis object captures sequence analysis results including sequence alignments, sequence variations and sequence annotations.",
    "required": [
        "analysisType"
    ],
    "properties": {
        "title": {
            "title": "Analysis Title",
            "description": "Short text that can be used to define submissions in searches or in displays.",
            "type": "string"
        },
        "description": {
            "title": "Analysis Description",
            "description": "Free-form text describing the data sets.",
            "type": "string"
        },
        "experimentRef": {
            "title": "Experiment Identifier (Accession ID or title)",
            "description": "Identifies the associated experiment.",
            "$ref": "#/definitions/reference"
        },
        "studyRef": {
            "title": "Study Identifier (Accession ID or title)",
            "description": "Identifies the associated study.",
            "$ref": "#/definitions/reference"
        },
        "sampleRef": {
            "title": "Sample Identifier (Accession ID or title)",
            "description": "Identifies the associated sample.",
            "$ref": "#/definitions/reference"
        },
        "runRef": {
            "title": "Run Identifier (Accession ID or title)",
            "description": "Identifies the associated run.",
            "$ref": "#/definitions/reference"
        },
        "analysisRef": {
            "title": "Analysis Identifier (Accession ID or title)",
            "description": "Identifies the associated analysis.",
            "$ref": "#/definitions/reference"
        },
        "analysisType": {
            "type": "object",
            "title": "Analysis Type",
            "additionalProperties": true,
            "properties": {
                "referenceAlignment": {
                    "type": "object",
                    "title": "Reference Alignment",
                    "additionalProperties": true,
                    "properties": {
                        "sequence": {
                            "type": "array",
                            "title": "Sequence",
                            "additionalItems": true,
                            "items": {
                                "type": "object",
                                "required": [
                                    "accessionId"
                                ],
                                "properties": {
                                    "label": {
                                        "type": "string",
                                        "title": "Label"
                                    },
                                    "refname": {
                                        "type": "string",
                                        "title": "Reference Name"
                                    },
                                    "accessionId": {
                                        "type": "string",
                                        "title": "Accession Id"
                                    }
                                }
                            }
                        }
                    }
                },
                "sequenceAssembly": {
                    "type": "object",
                    "title": "SequenceAssembly",
                    "properties": {
                        "name": {
                            "type": "string",
                            "title": "Name"
                        },
                        "partial": {
                            "type": "boolean",
                            "title": "Partial"
                        },
                        "coverage": {
                            "type": "string",
                            "title": "Coverage"
                        },
                        "program": {
                            "type": "string",
                            "title": "Program"
                        },
                        "platform": {
                            "type": "string",
                            "title": "Platform"
                        },
                        "minGapLength": {
                            "type": "number",
                            "title": "MinGapLength"
                        }
                    }
                },
                "sequenceVariation": {
                    "title": "Sequence Variation",
                    "$ref": "#/definitions/sequenceType"
                },
                "processedReads": {
                    "title": "Processed Reads",
                    "$ref": "#/definitions/sequenceType"
                }
            }
        },
        "files": {
            "type": "array",
            "title": "Files",
            "items": {
                "$ref": "#/definitions/file"
            }
        },
        "analysisLinks": {
            "type": "object",
            "title": "Analysis Links",
            "properties": {
                "xrefLinks": {
                    "$ref": "#/definitions/xrefLink"
                },
                "entrezLinks": {
                    "$ref": "#/definitions/entrezLink"
                },
                "urlLinks": {
                    "$ref": "#/definitions/urlLink"
                }
            }
        },
        "analysisAttributes": {
            "type": "array",
            "title": "Analysis Attributes",
            "items": {
                "$ref": "#/definitions/analysisAttribute"
            }
        }
    }
}
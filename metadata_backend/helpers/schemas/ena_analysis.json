{
    "title": "Analysis",
    "definitions": {
        "Links": {
            "$id": "#/definitions/Links",
            "title": "Link Type",
            "oneOf": [
                {
                    "$ref": "#/definitions/xrefLink"
                },
                {
                    "$ref": "#/definitions/urlLink"
                },
                {
                    "$ref": "#/definitions/entrezLink"
                }
            ]
        },
        "xrefLink": {
            "$id": "#/definitions/xrefLink",
            "title": "XRef Link",
            "type": "object",
            "required": [
                "xrefDb",
                "xrefId"
            ],
            "properties": {
                "xrefDb": {
                    "type": "string",
                    "title": "Database",
                    "description": "Name of the database or a link where the resource is located at. INSDC controlled vocabulary of permitted cross references.Please see http://www.insdc.org/db_xref.html . For example, FLYBASE."
                },
                "xrefId": {
                    "type": "string",
                    "title": "Database ID",
                    "description": "Accession ID or stable identifier, in the referenced database. For example, FBtr0080008 (in FLYBASE)"
                },
                "label": {
                    "type": "string",
                    "title": "Label",
                    "description": "A text label to identify the xrefLink by."
                }
            }
        },
        "urlLink": {
            "$id": "#/definitions/urlLink",
            "title": "URL Link",
            "type": "object",
            "required": [
                "label",
                "url"
            ],
            "properties": {
                "label": {
                    "type": "string",
                    "title": "Label",
                    "description": "A text label to identify the urlLink by."
                },
                "url": {
                    "type": "string",
                    "title": "URL",
                    "description": "The internet service link (http(s), ftp) etc.",
                    "pattern": "^(https?|ftp)://"
                }
            }
        },
        "entrezLink": {
            "$id": "#/definitions/entrezLink",
            "title": "Entrez Link",
            "type": "object",
            "required": [
                "entrezDb",
                "entrezId"
            ],
            "properties": {
                "entrezDb": {
                    "description": "Entrez Molecular Sequence Database System, NCBI controlled vocabulary of permitted cross references.",
                    "title": "Database",
                    "type": "string",
                    "enum": [
                        "annotinfo",
                        "assembly",
                        "biocollections",
                        "bioproject",
                        "biosample",
                        "biosystems",
                        "blastdbinfo",
                        "books",
                        "cdd",
                        "clinvar",
                        "dbvar",
                        "gap",
                        "gapplus",
                        "gds",
                        "gene",
                        "genome",
                        "geoprofiles",
                        "grasp",
                        "gtr",
                        "homologene",
                        "ipg",
                        "medgen",
                        "mesh",
                        "ncbisearch",
                        "nlmcatalog",
                        "nuccore",
                        "nucleotide",
                        "omim",
                        "orgtrack",
                        "pcassay",
                        "pccompound",
                        "pcsubstance",
                        "pmc",
                        "popset",
                        "protein",
                        "proteinclusters",
                        "protfam",
                        "pubmed",
                        "seqannot",
                        "snp",
                        "sra",
                        "structure",
                        "taxonomy"
                    ]
                },
                "entrezId": {
                    "type": "string",
                    "title": "Database ID",
                    "description": "Numeric record id meaningful to the NCBI Entrez system.",
                    "pattern": "^[a-zA-Z0-9]+"
                },
                "label": {
                    "description": "A text label to identify the url Entrez link by.",
                    "title": "Label",
                    "type": "string"
                }
            }
        },
        "analysisAttribute": {
            "$id": "#/definitions/analysisAttribute",
            "type": "object",
            "title": "Analysis Attribute",
            "required": [
                "tag",
                "value"
            ],
            "properties": {
                "tag": {
                    "type": "string",
                    "title": "Tag title"
                },
                "value": {
                    "type": "string",
                    "title": "Description"
                },
                "units": {
                    "type": "string",
                    "title": "Optional scientific units."
                }
            }
        },
        "sequenceType": {
            "$id": "#/definitions/sequenceType",
            "type": "object",
            "properties": {
                "assembly": {
                    "type": "object",
                    "title": "Reference assembly details",
                    "oneOf": [
                        {
                            "type": "object",
                            "title": "Standard",
                            "required": [
                                "accessionId"
                            ],
                            "properties": {
                                "refname": {
                                    "type": "string",
                                    "title": "Reference name"
                                },
                                "accessionId": {
                                    "type": "string",
                                    "title": "Accession Id"
                                }
                            }
                        },
                        {
                            "type": "object",
                            "title": "Custom",
                            "required": [
                                "description"
                            ],
                            "properties": {
                                "label": {
                                    "type": "string",
                                    "title": "Label",
                                    "description": "Text label to display for the link."
                                },
                                "url": {
                                    "type": "string",
                                    "title": "URL",
                                    "description": "The internet service link (http(s), ftp) etc.",
                                    "pattern": "^(https?|ftp)://"
                                },
                                "description": {
                                    "type": "string",
                                    "title": "Description"
                                }
                            }
                        }
                    ]
                },
                "sequence": {
                    "type": "array",
                    "title": "Sequence",
                    "items": {
                        "type": "object",
                        "required": [
                            "accessionId"
                        ],
                        "additionalProperties": true,
                        "properties": {
                            "label": {
                                "type": "string",
                                "title": "Label"
                            },
                            "accessionId": {
                                "type": "string",
                                "title": "Accession Id"
                            }
                        }
                    }
                }
            }
        },
        "identifiers": {
            "$id": "#/definitions/identifiers",
            "title": "Identifiers",
            "type": "object",
            "properties": {
                "primaryId": {
                    "type": "string",
                    "title": "Primary Id"
                },
                "secondaryId": {
                    "type": "string",
                    "title": "Secondary Id"
                },
                "uuid": {
                    "type": "string",
                    "title": "UUID"
                },
                "externalId": {
                    "type": "object",
                    "title": "External Id",
                    "properties": {
                        "namespace": {
                            "type": "string",
                            "title": "Namespace"
                        },
                        "value": {
                            "type": "string",
                            "title": "Identifier"
                        }
                    }
                },
                "submitterId": {
                    "type": "object",
                    "title": "Submitter Id",
                    "properties": {
                        "namespace": {
                            "type": "string",
                            "title": "Namespace"
                        },
                        "value": {
                            "type": "string",
                            "title": "Identifier"
                        }
                    }
                }
            }
        },
        "reference": {
            "$id": "#/definitions/reference",
            "additionalProperties": true,
            "type": "object",
            "properties": {
                "label": {
                    "type": "string",
                    "title": "Label"
                },
                "refname": {
                    "type": "string",
                    "title": "Record name"
                },
                "refcenter": {
                    "type": "string",
                    "title": "Center Namespace"
                },
                "accessionId": {
                    "type": "string",
                    "title": "Accession Id"
                },
                "identifiers": {
                    "$ref": "#/definitions/identifiers"
                }
            }
        },
        "file": {
            "$id": "#/definitions/file",
            "type": "object",
            "title": "File",
            "required": [
                "filename",
                "filetype",
                "checksumMethod",
                "checksum"
            ],
            "properties": {
                "filename": {
                    "type": "string",
                    "title": "Filename"
                },
                "filetype": {
                    "type": "string",
                    "title": "Filetype",
                    "enum": [
                        "agp",
                        "bai",
                        "bam",
                        "bcf_aggregate",
                        "bcf",
                        "bed",
                        "BioNano_native",
                        "chromosome_list",
                        "crai",
                        "cram",
                        "csi",
                        "fasta",
                        "fastq",
                        "flatfile",
                        "gff",
                        "info",
                        "Kallisto_native",
                        "manifest",
                        "other",
                        "phenotype_file",
                        "readme_file",
                        "sample_list",
                        "tab",
                        "tabix",
                        "unlocalised_list",
                        "vcf_aggregate",
                        "vcf",
                        "wig"
                    ]
                },
                "checksumMethod": {
                    "type": "string",
                    "title": "Checksum Method",
                    "enum": [
                        "MD5",
                        "SHA-256"
                    ]
                },
                "checksum": {
                    "type": "string",
                    "title": "Checksum"
                }
            }
        },
        "referenceAlignment": {
            "$id": "#/definitions/referenceAlignment",
            "type": "object",
            "title": "Reference Alignment",
            "required": [
                "referenceAlignment"
            ],
            "properties": {
                "referenceAlignment": {
                    "type": "object",
                    "title": "Reference Alignment",
                    "$ref": "#/definitions/sequenceType"
                }
            }
        },
        "sequenceAssembly": {
            "$id": "#/definitions/sequenceAssembly",
            "type": "object",
            "title": "Sequence Assembly",
            "required": [
                "sequenceAssembly"
            ],
            "properties": {
                "sequenceAssembly": {
                    "type": "object",
                    "title": "Sequence Assembly",
                    "properties": {
                        "name": {
                            "type": "string",
                            "title": "Name"
                        },
                        "coverage": {
                            "type": "string",
                            "title": "Coverage"
                        },
                        "program": {
                            "type": "string",
                            "title": "Program"
                        },
                        "platform": {
                            "type": "string",
                            "title": "Platform"
                        },
                        "minGapLength": {
                            "type": "number",
                            "title": "MinGapLength"
                        },
                        "molType": {
                            "type": "string",
                            "title": "MOL Type",
                            "enum": [
                                "genomic DNA",
                                "genomic RNA",
                                "viral cRNA"
                            ]
                        },
                        "authors": {
                            "type": "string",
                            "title": "Authors"
                        },
                        "address": {
                            "type": "string",
                            "title": "Address"
                        },
                        "type": {
                            "type": "string",
                            "title": "Type of Sequence Assembly",
                            "enum": [
                                "binned metagenome",
                                "clinical isolate assembly",
                                "clone or isolate",
                                "COVID-19 outbreak",
                                "Environmental Single-Cell Amplified Genome (SAG)",
                                "Metagenome-Assembled Genome (MAG)",
                                "primary metagenome"
                            ]
                        },
                        "tpa": {
                            "type": "boolean",
                            "title": "TPA"
                        },
                        "partial": {
                            "type": "boolean",
                            "title": "Partial"
                        }
                    }
                }
            }
        },
        "sequenceVariation": {
            "$id": "#/definitions/sequenceVariation",
            "type": "object",
            "title": "Sequence Variation",
            "required": [
                "sequenceVariation"
            ],
            "properties": {
                "sequenceVariation": {
                    "type": "object",
                    "title": "Sequence Variation",
                    "properties": {
                        "program": {
                            "type": "string",
                            "title": "Program"
                        },
                        "platform": {
                            "type": "string",
                            "title": "Platform"
                        },
                        "experimentType": {
                            "type": "string",
                            "title": "Experiment Type",
                            "enum": [
                                "Curation",
                                "Exome sequencing",
                                "Genotyping by array",
                                "Genotyping by sequencing",
                                "Target sequencing",
                                "transcriptomics",
                                "Whole genome sequencing",
                                "Whole transcriptome sequencing"
                            ]
                        },
                        "imputation": {
                            "type": "boolean",
                            "title": "imputation"
                        }
                    },
                    "assembly": {
                        "type": "object",
                        "title": "Reference assembly details",
                        "oneOf": [
                            {
                                "type": "object",
                                "title": "Standard",
                                "properties": {
                                    "refname": {
                                        "type": "string",
                                        "title": "Reference name"
                                    },
                                    "accessionId": {
                                        "type": "string",
                                        "title": "Accession Id"
                                    }
                                }
                            },
                            {
                                "type": "object",
                                "title": "Custom",
                                "properties": {
                                    "label": {
                                        "type": "string",
                                        "title": "Label",
                                        "description": "Text label to display for the link."
                                    },
                                    "url": {
                                        "type": "string",
                                        "title": "URL",
                                        "description": "The internet service link (http(s), ftp) etc.",
                                        "pattern": "^(https?|ftp)://"
                                    },
                                    "description": {
                                        "type": "string",
                                        "title": "Description"
                                    }
                                }
                            }
                        ]
                    },
                    "sequence": {
                        "type": "array",
                        "title": "Sequence",
                        "items": {
                            "type": "object",
                            "required": [
                                "accessionId"
                            ],
                            "additionalProperties": true,
                            "properties": {
                                "label": {
                                    "type": "string",
                                    "title": "Label"
                                },
                                "accessionId": {
                                    "type": "string",
                                    "title": "Accession Id"
                                }
                            }
                        }
                    }
                }
            }
        },
        "sequenceFlatFile": {
            "$id": "#/definitions/sequenceFlatFile",
            "type": "object",
            "title": "Sequence Flat File",
            "required": [
                "sequenceFlatFile"
            ],
            "properties": {
                "sequenceFlatFile": {
                    "type": "object",
                    "title": "Sequence Flat File",
                    "properties": {
                        "authors": {
                            "type": "string",
                            "title": "Authors"
                        },
                        "address": {
                            "type": "string",
                            "title": "Address"
                        }
                    }
                }
            }
        },
        "sequenceAnnotation": {
            "$id": "#/definitions/sequenceAnnotation",
            "type": "object",
            "title": "Sequence Annotation",
            "required": [
                "sequenceAnnotation"
            ],
            "properties": {
                "sequenceAnnotation": {
                    "type": "object",
                    "title": "Sequence Annotation",
                    "$ref": "#/definitions/sequenceType"
                }
            }
        },
        "processedReads": {
            "$id": "#/definitions/processedReads",
            "type": "object",
            "title": "Processed Reads",
            "required": [
                "processedReads"
            ],
            "properties": {
                "processedReads": {
                    "type": "object",
                    "title": "Processed Reads",
                    "$ref": "#/definitions/sequenceType"
                }
            }
        },
        "referenceSequence": {
            "$id": "#/definitions/referenceSequence",
            "type": "object",
            "title": "Reference Sequence",
            "required": [
                "referenceSequence"
            ],
            "properties": {
                "referenceSequence": {
                    "type": "object",
                    "title": "Reference Sequence",
                    "additionalProperties": true,
                    "properties": {
                        "description": {
                            "type": "string",
                            "title": "Sample Phenotype Description"
                        }
                    }
                }
            }
        },
        "samplePhenotype": {
            "$id": "#/definitions/samplePhenotype",
            "type": "object",
            "title": "Sample Phenotype",
            "required": [
                "samplePhenotype"
            ],
            "properties": {
                "samplePhenotype": {
                    "type": "object",
                    "title": "Sample Phenotype",
                    "additionalProperties": true,
                    "properties": {
                        "description": {
                            "type": "string",
                            "title": "Sample Phenotype Description"
                        }
                    }
                }
            }
        },
        "genomeMap": {
            "$id": "#/definitions/genomeMap",
            "type": "object",
            "title": "Genome Map",
            "required": [
                "genomeMap"
            ],
            "properties": {
                "genomeMap": {
                    "type": "object",
                    "title": "Genome Map",
                    "properties": {
                        "program": {
                            "type": "string",
                            "title": "Program"
                        },
                        "description": {
                            "type": "string",
                            "title": "Description"
                        },
                        "platform": {
                            "type": "string",
                            "title": "Platform",
                            "enum": [
                                "BioNano"
                            ]
                        }
                    }
                }
            }
        },
        "amrAntibiogram": {
            "$id": "#/definitions/amrAntibiogram",
            "type": "object",
            "title": "AMR Antibiogram",
            "required": [
                "amrAntibiogram"
            ],
            "properties": {
                "amrAntibiogram": {
                    "type": "object",
                    "title": "AMR Antibiogram",
                    "additionalProperties": true,
                    "properties": {
                        "description": {
                            "type": "string",
                            "title": "AMR Antibiogram Description"
                        }
                    }
                }
            }
        },
        "pathogenAnalysis": {
            "$id": "#/definitions/pathogenAnalysis",
            "type": "object",
            "title": "Pathogen Analysis",
            "required": [
                "pathogenAnalysis"
            ],
            "properties": {
                "pathogenAnalysis": {
                    "type": "object",
                    "title": "Pathogen Analysis",
                    "additionalProperties": true,
                    "properties": {
                        "description": {
                            "type": "string",
                            "title": "Pathogen Analysis Description"
                        }
                    }
                }
            }
        },
        "transcriptomeAssembly": {
            "$id": "#/definitions/transcriptomeAssembly",
            "type": "object",
            "title": "Transcriptome Assembly",
            "required": [
                "transcriptomeAssembly"
            ],
            "properties": {
                "transcriptomeAssembly": {
                    "type": "object",
                    "title": "Transcriptome Assembly",
                    "properties": {
                        "name": {
                            "type": "string",
                            "title": "Name"
                        },
                        "program": {
                            "type": "string",
                            "title": "Program"
                        },
                        "platform": {
                            "type": "string",
                            "title": "Platform"
                        },
                        "authors": {
                            "type": "string",
                            "title": "Authors"
                        },
                        "address": {
                            "type": "string",
                            "title": "Address"
                        },
                        "type": {
                            "type": "string",
                            "title": "Type of Transcriptome Assembly",
                            "description": "An assembly of reads from an isolated cultured organism, tissues, cells or a cell line. An original metatranscriptome assembly from a sampled biome or collection of sampled biomes without attempt to separate taxa.",
                            "enum": [
                                "isolate",
                                "metatranscriptome"
                            ]
                        },
                        "tpa": {
                            "type": "boolean",
                            "title": "TPA"
                        }
                    }
                }
            }
        },
        "taxonomicReferenceSet": {
            "$id": "#/definitions/taxonomicReferenceSet",
            "type": "object",
            "title": "Taxonomic Reference Set",
            "required": [
                "taxonomicReferenceSet"
            ],
            "properties": {
                "taxonomicReferenceSet": {
                    "type": "object",
                    "title": "Taxonomic Reference Set",
                    "properties": {
                        "name": {
                            "type": "string",
                            "title": "Name"
                        },
                        "taxonomySystem": {
                            "type": "string",
                            "title": "Taxonomy System"
                        },
                        "taxonomySystemVersion": {
                            "type": "string",
                            "title": "Taxonomy System Version"
                        },
                        "customFields": {
                            "type": "array",
                            "title": "Custom Fields",
                            "items": {
                                "type": "object",
                                "title": "Field",
                                "additionalProperties": true,
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "title": "Name"
                                    },
                                    "description": {
                                        "type": "string",
                                        "title": "Description"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "assemblyAnnotation": {
            "$id": "#/definitions/assemblyAnnotation",
            "type": "object",
            "title": "Assembly Annotation",
            "required": [
                "assemblyAnnotation"
            ],
            "properties": {
                "assemblyAnnotation": {
                    "type": "object",
                    "title": "Assembly Annotation",
                    "$ref": "#/definitions/sequenceType"
                }
            }
        }
    },
    "type": "object",
    "description": "A SRA analysis object captures sequence analysis results including sequence alignments, sequence variations and sequence annotations.",
    "required": [
        "analysisType"
    ],
    "properties": {
        "title": {
            "title": "Analysis Title",
            "description": "Short text that can be used to define submissions in searches or in displays.",
            "type": "string"
        },
        "analysisType": {
            "type": "object",
            "title": "Analysis Type",
            "oneOf": [
                {
                    "$ref": "#/definitions/referenceAlignment"
                },
                {
                    "$ref": "#/definitions/sequenceVariation"
                },
                {
                    "$ref": "#/definitions/sequenceAssembly"
                },
                {
                    "$ref": "#/definitions/sequenceFlatFile"
                },
                {
                    "$ref": "#/definitions/sequenceAnnotation"
                },
                {
                    "$ref": "#/definitions/referenceSequence"
                },
                {
                    "$ref": "#/definitions/samplePhenotype"
                },
                {
                    "$ref": "#/definitions/processedReads"
                },
                {
                    "$ref": "#/definitions/amrAntibiogram"
                },
                {
                    "$ref": "#/definitions/pathogenAnalysis"
                },
                {
                    "$ref": "#/definitions/transcriptomeAssembly"
                },
                {
                    "$ref": "#/definitions/taxonomicReferenceSet"
                },
                {
                    "$ref": "#/definitions/assemblyAnnotation"
                }
            ]
        },
        "analysisDate": {
            "title": "Analysis Date",
            "description": "Date when the analysis took place",
            "type": "string",
            "format": "date-time"
        },
        "analysisCenter": {
            "title": "Analysis Center",
            "description": "If applicable, the name of the contract sequencing center that executed the analysis.",
            "type": "string"
        },
        "description": {
            "title": "Analysis Description",
            "description": "Free-form text describing the data sets.",
            "type": "string"
        },
        "studyRef": {
            "title": "Study Reference",
            "description": "Identifies the associated study.",
            "$ref": "#/definitions/reference"
        },
        "experimentRef": {
            "title": "Experiment Reference",
            "description": "Identifies the associated experiment.",
            "type": "array",
            "items": {
                "$ref": "#/definitions/reference"
            }
        },
        "sampleRef": {
            "title": "Sample Reference",
            "description": "Identifies the associated sample.",
            "type": "array",
            "items": {
                "$ref": "#/definitions/reference"
            }
        },
        "runRef": {
            "title": "Run Reference",
            "description": "Identifies the associated run.",
            "type": "array",
            "items": {
                "$ref": "#/definitions/reference"
            }
        },
        "analysisRef": {
            "title": "Analysis Reference",
            "description": "Identifies the associated analysis.",
            "type": "array",
            "items": {
                "$ref": "#/definitions/reference"
            }
        },
        "files": {
            "type": "array",
            "title": "Files",
            "items": {
                "$ref": "#/definitions/file"
            }
        },
        "analysisLinks": {
            "type": "array",
            "title": "Analysis Links",
            "description": "Links to resources related to this experiment or experiment set (publication, datasets, online databases). Used to encode URL links, Entrez links, and xref DB links. ",
            "items": {
                "$ref": "#/definitions/Links"
            }
        },
        "analysisAttributes": {
            "type": "array",
            "title": "Analysis Attributes",
            "description": "Properties and attributes of the data set. These can be entered as free-form tag-value pairs. Submitters may be asked to follow a community established ontology when describing the work.",
            "items": {
                "$ref": "#/definitions/analysisAttribute"
            }
        }
    }
}
    "type": "object",
    "description": "A SRA analysis object captures sequence analysis results including sequence alignments, sequence variations and sequence annotations.",
    "required": [
        "analysisType"
    ],
    "properties": {
        "title": {
            "title": "Analysis Title",
            "description": "Short text that can be used to define submissions in searches or in displays.",
            "type": "string"
        },
        "analysisType": {
            "type": "object",
            "title": "Analysis Type",
            "oneOf": [
                {
                    "$ref": "#/definitions/referenceAlignment"
                },
                {
                    "$ref": "#/definitions/sequenceVariation"
                },
                {
                    "$ref": "#/definitions/sequenceAssembly"
                },
                {
                    "$ref": "#/definitions/sequenceFlatFile"
                },
                {
                    "$ref": "#/definitions/sequenceAnnotation"
                },
                {
                    "$ref": "#/definitions/referenceSequence"
                },
                {
                    "$ref": "#/definitions/samplePhenotype"
                },
                {
                    "$ref": "#/definitions/processedReads"
                },
                {
                    "$ref": "#/definitions/amrAntibiogram"
                },
                {
                    "$ref": "#/definitions/pathogenAnalysis"
                },
                {
                    "$ref": "#/definitions/transcriptomeAssembly"
                },
                {
                    "$ref": "#/definitions/taxonomicReferenceSet"
                },
                {
                    "$ref": "#/definitions/assemblyAnnotation"
                }
            ]
        },
        "analysisDate": {
            "title": "Analysis Date",
            "description": "Date when the analysis took place",
            "type": "string",
            "format": "date-time"
        },
        "analysisCenter": {
            "title": "Analysis Center",
            "description": "If applicable, the name of the contract sequencing center that executed the analysis.",
            "type": "string"
        },
        "description": {
            "title": "Analysis Description",
            "description": "Free-form text describing the data sets.",
            "type": "string"
        },
        "studyRef": {
            "title": "Study Reference",
            "description": "Identifies the associated parent study.",
            "$ref": "#/definitions/reference"
        },
        "experimentRef": {

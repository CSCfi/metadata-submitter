                "samplePhenotype"
            ],
            "properties": {
                "samplePhenotype": {
                    "type": "object",
                    "title": "Sample Phenotype",
                    "additionalProperties": true,
                    "properties": {
                        "description": {
                            "type": "string",
                            "title": "Sample Phenotype Description"
                        }
                    }
                }
            }
        },
